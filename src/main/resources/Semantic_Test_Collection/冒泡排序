/*冒泡排序*/
/*
啦啦啦
*/
int p[10];
int a;
a = 0;
while(a < 10)
{
  read(p[a]);
  a = a + 1;
}


int p[5] = {3,1,90,23,-9};
int i, n, pos,a;
n = 5;
i = 0;
while(i < n)
{
    pos = i + 1;
    while(pos < n)
    {
        if(p[pos] > p[i])
        {
            a = p[pos];
            p[pos] = p[i];
            p[i] = a;
        }
        pos = pos + 1;
    }
    i = i + 1;
}

-----
int a = 1|| 9-9;
-----
a>0&& p[a]

write("The array after sorting is:#n#");//#n#为换行符
i = 0;
while(i <= 9)
{
    write(p[i]);
    write(" ");
    i = i + 1;
}
-----
//插入排序
int p = -1;
int a[2] = {2,100};
if(p >0 && a[p])
    p = 1000;
else
    p = 2000;

-----
print(1);